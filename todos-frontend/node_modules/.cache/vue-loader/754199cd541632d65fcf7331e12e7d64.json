 <TodoCardVue :todo=\"todo\" v-on:onRemove=\"load\" class=\"w-100 mt-3\" v-for=\"(todo) in todos\" v-bind:key=\"todo.id\"/>\n            </div>\n        </b-container>\n    </div>\n</template>\n\n<script>\nimport TodoCardVue from './TodoCard.vue'\nimport AddTodoVue from './AddTodo.vue'\nimport axios from 'axios'\n\nexport default {\n    data() {\n        return {\n            newTitle: \"\",\n            todos: []\n        }\n    },\n    mounted() {\n        console.log('mounted')\n        this.load()\n    },\n    methods: {\n        add() {\n            axios\n            .post('http://localhost:8080/todos', {\n                headers: {\n                    \"Authorization\": \"Bearer \" + this.user.token\n                }\n            })\n            .then(response => console.log(response));\n        },\n        load() {\n            axios\n            .get('http://localhost:8080/todos', {\n                headers: {\n                    \"Authorization\": \"Bearer \" + this.user.token\n                }\n            })\n            .then(object => { \n                this.todos = JSON.parse(object.request.response)\n            });\n        },\n        logout() {\n            localStorage.removeItem('user')\n            this.$emit('onLogout')\n        }\n    },\n    components: {\n        AddTodoVue,\n        TodoCardVue\n    },\n    computed: {\n        user() {\n            return JSON.parse(localStorage.getItem('user'))\n        }\n    }\n}\n</script>\n\n<style>\n</style>"]}]}