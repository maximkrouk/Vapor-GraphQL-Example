-flex flex-column\">\n                <b-form-input class=\"mt-2\" v-model=\"input.username\" placeholder=\"Username\"></b-form-input>\n                <b-form-input class=\"mt-2\" v-model=\"input.password\" placeholder=\"Password\"></b-form-input>\n                <b-button variant=\"primary\" class=\"w-75 mt-2 mx-auto\" @click=\"login\">Register</b-button>\n                <b-button @click=\"toRegister\" variant=\"outline-primary\" class=\"w-75 mt-2 mx-auto\">Go to register</b-button>\n            </div>\n        </div>\n    </b-container>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            input: {\n                username: \"\",\n                password: \"\",\n            },\n            helper: {\n                confirmPassword: \"\",\n                initialState: true\n            }\n        }\n    },\n    methods: {\n        login() {\n            fetch(\"http://localhost:8080/users/login\", {\n                method: 'POST',\n                headers: {\n                    'Content-Type': 'application/json'\n                },\n                body: JSON.stringify(this.input)\n            })\n            .then((request) => request.json())\n            .then((json) => localStorage.setItem('user', JSON.stringify(json)))\n        },\n        toRegister() {\n            this.$emit(\"toRegister\")\n        }\n    },\n    computed: {\n        user() {\n            return JSON.parse(localStorage.getItem('user'))\n        }\n    }\n}\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components"}]}