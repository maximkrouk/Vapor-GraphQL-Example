>\nimport TodoCardVue from './TodoCard.vue'\nimport AddTodoVue from './AddTodo.vue'\n\nexport default {\n    data() {\n        return {\n            newTitle: \"\",\n            todos: []\n        }\n    },\n    methods: {\n        add() {\n\n        },\n        load() {\n            console.log(\"send\")\n            var xhr = new XMLHttpRequest();\n\n            xhr.onreadystatechange = () => {\n                console.log(xhr.responseText)\n                if (xhr.readyState != 4) return\n                this.todos = xhr.response\n            }\n            xhr.open('GET', 'http://localhost:8080/todos', true);\n            xhr.setRequestHeader(\"Authorization\", \"Bearer \" + this.user.token);\n            xhr.send()\n        },\n        logout() {\n            localStorage.removeItem('user')\n        }\n    },\n    components: {\n        AddTodoVue,\n        TodoCardVue\n    },\n    computed: {\n        user() {\n            return JSON.parse(localStorage.getItem('user'))\n        }\n    }\n}\n</script>\n\n<style>\n</style>"]}]}